{
  "name": "@csstools/postcss-text-decoration-shorthand",
  "description": "Use text-decoration in it's shorthand form in CSS",
  "version": "1.0.0",
  "license": "CC0-1.0",
  "funding": {
    "type": "opencollective",
    "url": "https://opencollective.com/csstools"
  },
  "engines": {
    "node": "^12 || ^14 || >=16"
  },
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
