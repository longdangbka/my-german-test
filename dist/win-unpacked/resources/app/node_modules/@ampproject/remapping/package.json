  "engines": {
    "node": ">=6.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-typescript": "8.3.2",
    "@types/jest": "27.4.1",
    "@typescript-eslint/eslint-plugin": "5.20.0",
    "@typescript-eslint/parser": "5.20.0",
    "eslint": "8.14.0",
    "eslint-config-prettier": "8.5.0",
    "jest": "27.5.1",
    "jest-config": "27.5.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.6.2",
    "rollup": "2.70.2",
    "ts-jest": "27.1.4",
    "tslib": "2.4.0",
    "typescript": "4.6.3"
  },
  "dependencies": {
    "@jridgewell/gen-mapping": "^0.3.5",
    "@jridgewell/trace-mapping": "^0.3.24"
  }
