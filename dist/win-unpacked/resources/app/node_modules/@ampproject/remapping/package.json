      "./dist/remapping.umd.js"
    ],
    "./package.json": "./package.json"
  },
  "files": [
    "dist"
  ],
  "author": "Justin Ridgewell <jridgewell@google.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ampproject/remapping.git"
